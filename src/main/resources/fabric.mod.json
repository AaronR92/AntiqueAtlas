{
  "schemaVersion": 1,
  "id": "antiqueatlas",
  "name": "Antique Atlas",
  "description": "Antique Atlas is a book that acts like a map featuring infinite scrolling, zoom and custom labeled markers. The map is generated around the player by calculating the average biome in each 16x16 chunk.",
  "version": "${version}",
  "entrypoints": {
    "main": [
      "hunternif.mc.impl.atlas.AntiqueAtlasMod"
    ],
    "client": [
      "hunternif.mc.impl.atlas.AntiqueAtlasModClient"
    ]
  },
  "environment": "*",
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/antique-atlas",
    "sources": "https://github.com/AntiqueAtlasTeam/AntiqueAtlas",
    "issues": "https://github.com/AntiqueAtlasTeam/AntiqueAtlas/issues"
  },
  "icon": "antiqueatlas_icon.png",
  "depends": {
    "fabricloader": ">=0.10.8",
    "fabric": ">=0.25.0",
    "minecraft": ["1.16.2", "1.16.3", "1.16.4"]
  },
  "accessWidener": "antiqueatlas.accesswidener",
  "mixins": [
    "antiqueatlas.mixins.json",
    "antiqueatlasoverlay.mixins.json"
  ]
}
